    {
      "type": "Microsoft.Compute/virtualMachines",
      "apiVersion": "2024-11-01",
      "name": "[parameters('virtualMachines_WEB1_name')]",
      "location": "eastus2",
      "dependsOn": [
        "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_web1192_z1_name'))]"
      ],
